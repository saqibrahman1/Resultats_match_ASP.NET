@model scoreGr03.Models.But

@{
    ViewData["Title"] = "Supprimer un But";
}

<h1>Supprimer un But</h1>


<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Temps)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Temps)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Joueur)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Joueur.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Match)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Match.Id)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" id="id" asp-for="Id" />
        <input type="submit" id="supprimerbtbut" value="Supprimer" class="btn btn-danger" /> |
        <a asp-action="Index">Retour au listes</a>
    </form>
</div>
@section Scripts {
    <script src="~/js/signalr/dist/browser/signalr.js"></script>
    <script src="~/js/DeleteButs.js"></script> }